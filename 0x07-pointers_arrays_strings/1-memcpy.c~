#include "main.h"
/**
 * _memset - Entry point
 *
 *@s: pointer to be filled
 *@b: char to fill pointer with
 *@n: number of memory spaces to fill in s with character b
 *Return: pointer to memory area s
 */

char *_memset(char *s, char b, unsigned int n)
{
	unsigned int i = 0;

	for (i = 0; i < n; i++)
	{
		s[i] = b;
	}
	return (s);
}
