#include "main.h"
/**
 * _strchr - Entry point
 *
 *
 *@s: pointer to search in for character c
 *@c: character to search for
 *Return: position of character or NULL if character not found
 */

char *_strspn(char *s, char c)
{
	unsigned int i, j, k;

	for (i = 0; s[i] != '\0'; i++)
	{
		k = 1;
		for (j = 0; *(accept + j) != '\0'; j++)
		{
			if (*(s + i) == *(accept + j))
			{
				k = 0;
				break;
			}
		}
		if (k == 1)
			break;
	}
	return (i);
}
