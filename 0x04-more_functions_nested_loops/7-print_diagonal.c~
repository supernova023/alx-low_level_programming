#include <stdio.h>
#include "main.h"
/**
 * print_line - Extra point
 * Description - print line n characters long
 * @n: variable determines line length
 *
 *
 * Return
 */

void print_line(int n)
{
	int y;

	if (n <= 0)
	{
	}
	else
	{
		for (y = 0; y < n; y++)
		{
			_putchar('_');
		}
	}
	_putchar('\n');

}
